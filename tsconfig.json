{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "strict": true,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "target": "es6",
    "sourceMap": false,
    "rootDir": "./",
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "dist"]
}
